{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
        "*": [
            "node_modules/*",
            "src-broken/types/*"
        ]
    }
},
"include": [
    "./src"
]
}